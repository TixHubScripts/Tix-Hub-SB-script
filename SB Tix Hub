local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Tix Hub", "Ocean")

local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("SPAM")
MainSection:NewButton(" key binds Whirlwind", "spams Whirlwind controls: (R) (F) (G)", function() 





    local UIS = game:GetService("UserInputService")

    UIS.InputBegan:Connect(function(input)
     if input.UserInputType == Enum.UserInputType.Keyboard then
      if input.KeyCode == Enum.KeyCode.R then
    local Event = game:GetService("ReplicatedStorage").GeneralAbility
    Event:FireServer()
    
      end
     end
    end)
    
    UIS.InputBegan:Connect(function(input)
     if input.UserInputType == Enum.UserInputType.Keyboard then
      if input.KeyCode == Enum.KeyCode.F then
    Game.Players.LocalPlayer.Character.Humanoid.WalkSpeed =50
    
      end
     end
    end)
    
    
    UIS.InputBegan:Connect(function(input)
     if input.UserInputType == Enum.UserInputType.Keyboard then
      if input.KeyCode == Enum.KeyCode.G then
    Game.Players.LocalPlayer.Character.Humanoid.WalkSpeed =16
    
      end
     end
    end)
    
    
    
end)

MainSection:NewButton(" key bind CHEEKY", "spams CHEEKY control : (E)", function() 
    local UIS = game:GetService("UserInputService")

    UIS.InputBegan:Connect(function(input)
     if input.UserInputType == Enum.UserInputType.Keyboard then
      if input.KeyCode == Enum.KeyCode.E then
local Event = game:GetService("ReplicatedStorage").Spherify
Event:FireServer()

      end
     end
    end)
end)

MainSection:NewButton(" key bind diamond", "spams diamond control : (E)", function() 
    local UIS = game:GetService("UserInputService")

    UIS.InputBegan:Connect(function(input)
     if input.UserInputType == Enum.UserInputType.Keyboard then
      if input.KeyCode == Enum.KeyCode.E then
       local Event = game:GetService("ReplicatedStorage").Rockmode
    Event:FireServer()
      end
     end
    end)
end)

MainSection:NewButton(" key bind defense", "spams defense control : (E)", function() 
    local UIS = game:GetService("UserInputService")

    UIS.InputBegan:Connect(function(input)
     if input.UserInputType == Enum.UserInputType.Keyboard then
      if input.KeyCode == Enum.KeyCode.E then
    local Event = game:GetService("ReplicatedStorage").Barrier
    Event:FireServer()
      end
     end
    end)
end)

MainSection:NewButton(" key bind ZZZZZZ", "spams ZZZZZZ control : (E)", function() 
    local UIS = game:GetService("UserInputService")
    UIS.InputBegan:Connect(function(input)
     if input.UserInputType == Enum.UserInputType.Keyboard then
      if input.KeyCode == Enum.KeyCode.E then
    local Event = game:GetService("ReplicatedStorage").ZZZZZZZSleep
    Event:FireServer()
      end
     end
    end)
end)

local MainSection = Main:NewSection("OP")

MainSection:NewButton("Golden god mode", "press the button for godmode need golden glove", function() 

 
local A_1 = true
local Event = game:GetService("ReplicatedStorage").Goldify
Event:FireServer(A_1)

end)

local Farm = Window:NewTab("Farm")
local FarmSection = Farm:NewSection("Farm")

FarmSection:NewButton("get REDACTED", "press the button for godmode need golden glove", function() 
  local Door = 1

  if not game:GetService("BadgeService"):UserHasBadgeAsync(game.Players.LocalPlayer.UserId, 2124847850) and 5000 <= game.Players.LocalPlayer.leaderstats.Slaps.Value then
  
      repeat
      
      task.wait(.25)
  
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.PocketDimension.Doors[Door].CFrame
  
      Door = Door + 1
  
      print(Door)
  
      wait(5)
  
  
  
      
      until game:GetService("BadgeService"):UserHasBadgeAsync(game.Players.LocalPlayer.UserId, 2124847850)
  
  else 
  
      print("YOU ALREADY HAVE [ R E D A C T E D ] !!")
  
      print("OR YOU DONT HAVE 5K SLAPS")
  
  end

end)




local Safe = Window:NewTab("Safe")
local SafeSection = Safe:NewSection("Safe")

SafeSection:NewButton("Anti Admins", "Kicks u when admins join", function()
  if Plr:GetRankInGroup(9950771) and 7 <= Plr:GetRankInGroup(9950771) and antiAdmins then
    game.Players.LocalPlayer:Kick("RUNNNN ADMINS ARE COMINGGGG")
  end
end)
